        <item name="android:textColor">@color/colorTextWhite</item>
        <item name="android:textSize">@dimen/text_size_toolbar_title</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/base_title</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="NavigationTitleBoldStyle" parent="TextStyleBold">
